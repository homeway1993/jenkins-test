pipeline {
    agent any
    triggers {
        pollSCM('* * * * *')
    }
    stages {
        stage('Checkout') {
            steps {
                checkout([
                    $class: 'GitSCM',
                    userRemoteConfigs: 'https://github.com/homeway1993/simple-java-maven-app.git',
                    branches: []
                ])
            }
            post {
                always {
                    sh 'echo "Test post"'
                    // sh 'docker rm -f httpd'
                }
            }
        }
    }
}